[user]
	name = Felix Sch√§fer
	email = felix@thegcat.net
[push]
	default = simple
[credential]
	helper = osxkeychain
[core]
	editor = /usr/local/bin/vim
  excludesfile = /Users/thegcat/.gitignore_global
[alias]
  s = status
  a = add -p
  p = push
  ci = commit
  br = branch
  co = checkout
  cp = cherry-pick -e
  df = diff
  ds = diff --cached
  fixup = !sh -c 'git commit -m \"fixup! $(git log -1 --format='\\''%s'\\'' $@)\"' -
  squash = !sh -c 'git commit -m \"squash! $(git log -1 --format='\\''%s'\\'' $@)\"' -
  re = rebase --autosquash
  ri = rebase --interactive --autosquash
  rc = rebase --continue
  ra = rebase --abort
  sa = submodule add
  si = submodule init
  su = submodule update
  ss = submodule sync
  fp = push --force
  unstage = reset HEAD
  uncommit = reset --soft HEAD^
  stash = stash -u
  tree = log --graph --pretty=onelineverbose --abbrev-commit
[pull]
  rebase = true
[pretty]
  onelineverbose = "%C(yellow)%h%Creset (%Cblue%aN%Creset)%Cred%d%Creset %s [%Cgreen%cd, %ad%Creset]"
[color "diff"]
  meta = yellow bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[help]
  autocorrect = 5
